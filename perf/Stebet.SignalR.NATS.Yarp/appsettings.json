{
    "Logging": {
        "LogLevel": {
            "Default": "Warning",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*",
    "ReverseProxy": {
        "Routes": {
            "route1": {
                "ClusterId": "cluster1",
                "Match": {
                    "Path": "{**catch-all}"
                }
            }
        },
        "Clusters": {
            "cluster1": {
                "SessionAffinity": {
                    "Enabled": "true",
                    "AffinityKeyName":  "affinity"
                },
                "Destinations": {
                    "destination1": {
                        "Address": "http://localhost:5001"
                    },
                    "destination2": {
                        "Address": "http://localhost:5002"
                    }
                }
            }
        }
    }
}
